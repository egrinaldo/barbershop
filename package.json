{
  "name": "barbearia-agendamentos",
  "version": "1.0.0",
  "description": "Sistema de agendamento para barbearia solidária",
  "main": "backend/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/index.js",
    "dev:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "start": "node backend/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "seed": "node backend/scripts/seed.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "@prisma/client": "^5.6.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "emailjs-com": "^3.2.0"
  },
  "devDependencies": {
    "prisma": "^5.6.0",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "barbearia",
    "agendamento",
    "react",
    "tailwind",
    "prisma"
  ],
  "author": "Barbearia Solidária",
  "license": "MIT"
}